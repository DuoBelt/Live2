var sqlstmt;var method;var conn;var output = {};output.msg = "";$.response.contentType = "text/json";$.response.status = $.net.http.OK;method = parseInt($.request.parameters.get("method"),10);if (method >= 1 && method <= 4) {    conn = $.db.getConnection();    sqlstmt = 'CREATE LOCAL TEMPORARY COLUMN TABLE #CONTROL ("NAME" VARCHAR(50), "INTARGS" INTEGER, "DOUBLEARGS" DOUBLE, "STRINGARGS" VARCHAR(100))';     conn.prepareStatement(sqlstmt).execute();    sqlstmt = 'INSERT INTO #CONTROL VALUES (\'THREAD_NUMBER\', 2, null, null)';    conn.prepareStatement(sqlstmt).execute();    sqlstmt = 'INSERT INTO #CONTROL VALUES (\'METHOD\', ?, null, null)';    var pstmt = conn.prepareStatement(sqlstmt);    pstmt.setInt(1,method);    pstmt.execute();    sqlstmt = 'INSERT INTO #CONTROL VALUES (\'BETA\', null, 0.005, null)';    conn.prepareStatement(sqlstmt).execute();    sqlstmt = 'INSERT INTO #CONTROL VALUES (\'MIN_SCORE\', null, 0.02, null)';    conn.prepareStatement(sqlstmt).execute();    sqlstmt = 'TRUNCATE TABLE "LIVE2"."Live2::lp.model_linkPrediction_Result"';    conn.prepareStatement(sqlstmt).execute();    sqlstmt = 'CALL "LIVE2"."Live2::lp.model"("LIVE2"."Connections", #CONTROL, "LIVE2"."Live2::lp.model_linkPrediction_Result") with overview';    conn.prepareCall(sqlstmt).execute();    conn.commit();    output.msg = "Finished!";} else {    output.msg = "Value for 'method' parameter is not between 1 and 4.";}if (conn) {conn.close();}    $.response.setBody(JSON.stringify(output));